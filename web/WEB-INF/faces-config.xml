<?xml version='1.0' encoding='UTF-8'?>
<faces-config version="2.2"
              xmlns="http://xmlns.jcp.org/xml/ns/javaee"
              xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
              xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd">
    <application> 
        <locale-config>
            <default-locale>tr</default-locale>
            <supported-locale>en</supported-locale>
        </locale-config>    	   
        <resource-bundle>
            <base-name>com.oms.language.bundle</base-name>
            <var>message</var>
        </resource-bundle>
        <resource-bundle>
            <base-name>com.oms.dbConfig.dbConfig</base-name>
            <var>dbConfig</var>
        </resource-bundle>
        <resource-bundle>
            <base-name>com.oms.dbConfig.serverConfig</base-name>
            <var>serverConfig</var>
        </resource-bundle>
     </application>
</faces-config>
